{
  "name": "rizwansaleem70/laravel-payfort",
  "description": "Payfort payment gateway integration with Laravel. Add Amazon Payment Service (PayFort) in your Laravel Application.",
  "keywords": [
    "payment",
    "payfort",
    "laravel amazon payment service",
    "amazon payment service",
    "laravel payfort"
  ],
  "authors": [
    {
      "name": "Rizwan Saleem",
      "email": "rizwansaleem70@gmail.com"
    }
  ],
  "minimum-stability" : "dev",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0",
    "nesbot/carbon": "^2.0",
    "guzzlehttp/guzzle": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "LaravelPayfort\\": "src/"
    }
  },
  "extra": {
    "laravel": {
        "providers": [
            "LaravelPayfort\\Providers\\PayfortServiceProvider"
        ],
        "aliases": {
            "Payfort": "LaravelPayfort\\Facades\\Payfort"
        }
    }
  }
}
